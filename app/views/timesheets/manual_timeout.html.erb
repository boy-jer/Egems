<p>Please input missing timeout</p>
<table class="table">
  <thead>
  <tr>
    <th>Date</th>
    <th>Time in</th>
    <th>Time out</th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><%= format_date @invalid_timesheet.date %></td>
    <td><%= format_time @invalid_timesheet.time_in %></td>
    <td>
    <%= form_for(@invalid_timesheet, :url => { :action => 'manual_timeout', :id => @invalid_timesheet.id }, :method => :post) do |f| %>
      <%= text_field 'timeout', 'hour', :placeholder => 'hh', :size => 2, :maxlength => 2, :class => 'time' %>
      <%= text_field 'timeout', 'min', :placeholder => 'mm', :size => 2, :maxlength => 2, :class => 'time' %>
      <%= select 'timeout', 'meridian', ['AM', 'PM'], {}, { :class => 'time' } %>
      <%= text_field 'timeout', 'date', :value => format_date(@invalid_timesheet.date), :class => 'time', :style => 'width: 100px;' %>
      <%= f.submit 'Timeout' %>
    <% end -%>
    </td>
  </tr>
  </tbody>
</table>
